    "num = int(input(\"enter the number:\"))\n",
    "sum =0\n",
    "a = num\n",
    "while num>0:\n",
    "    remainder = num%10\n",
    "    sum = sum + remainder*remainder*remainder\n",
    "    num = num//10\n",
    "    \n",
    "if a==sum:\n",
    "    print(\"it is an armstrong\")\n",
    "else:\n",
    "    print(\"it is not an armstrong\")"
