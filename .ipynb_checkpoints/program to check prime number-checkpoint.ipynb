    "# program to check prime number\n",
    "num = int(input(\"enter the number:\"))\n",
    "for i in range (2,num):\n",
    "    if (num % i)==0:\n",
    "        print(\"it is not prime\")\n",
    "        break\n",
    "else:\n",
    "        print(\"it is prime\")\n"
